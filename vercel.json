{
  "builds": [
    {
      "src": "react-project/my-finance-app/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "react-project/my-finance-app/frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "react-project/my-finance-app/frontend/dist/$1"
    }
  ]
}
